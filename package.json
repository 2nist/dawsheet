{
  "name": "dawsheet-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "proxy:dev": "cd apps/proxy-java && ./gradlew run",
    "gas:push": "cd apps/gas && clasp push",
  "infra:setup": "bash infra/create_topics.sh",
  "midi:build": "node tools/midi-build-temp.mjs",
  "midi:sync": "node tools/sync-midi-iife.mjs",
  "midi:sync:push": "node tools/sync-midi-iife.mjs --push",
  "importhub:bundle": "npm run midi:build && npm run midi:sync",
  "importhub:bundle:push": "npm run midi:build && npm run midi:sync:push"
  }
}
